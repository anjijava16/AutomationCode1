<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="TestSuiteRegistrationProcess" verbose="1"
	thread-count="1">
	<test name="TestSuiteRegistrationProcess" preserve-order="true">
		<classes>
			<class name="com.edureka.scripts.VerifySignUpFromNavigationHeader" />
			<class
				name="com.edureka.scripts.VerifyUserNotAbleToSignupIfMandatoryFieldIsEmpty" />
			<class
				name="com.edureka.scripts.VerifyUserNotAbleToSignUpIfEnteredPhNumberInAlphabets" />
			<class
				name="com.edureka.scripts.VerifyLoginWithCorrectEmailAndCorrectPassword" />
			<class
				name="com.edureka.scripts.VerifyUserCannotLoggedInWithIncorrectEmailAndCorrectPassword" />
		</classes>
	</test>
</suite> 